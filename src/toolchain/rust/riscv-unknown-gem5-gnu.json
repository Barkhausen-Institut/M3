{
  "arch": "riscv64",
  "os": "none",
  "vendor": "gem5",

  "cpu": "generic-rv64",
  "data-layout": "e-m:e-p:64:64-i64:64-i128:128-n64-S128",
  "llvm-target": "riscv64",
  "target-endian": "little",
  "target-pointer-width": "64",
  "target-c-int-width": "32",
  "linker-flavor": "gcc",
  "linker": "rust-lld",
  "max-atomic-width": "64",
  "atomic-cas": true,
  "features": "+m,+a,+f,+d,+c,",
  "executables": true,
  "relocation-model": "static",
  "frame-pointer": "always",
  "emit-debug-gdb-scripts": true
}
