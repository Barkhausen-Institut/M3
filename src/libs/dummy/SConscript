Import('env')
if env['TGT'] == 'gem5':
    lib = env.StaticLibrary(target = 'm', source = env.Glob('*.cc'))
    env.Install(env['LIBDIR'], lib)

    lib = env.StaticLibrary(target = 'stdc++', source = env.Glob('*.cc'))
    env.Install(env['LIBDIR'], lib)

    lib = env.StaticLibrary(target = 'gloss', source = env.Glob('*.cc'))
    env.Install(env['LIBDIR'], lib)
