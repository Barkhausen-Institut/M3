<config>
    <mods>
        <mod name="fs" file="bench.img" />
    </mods>
    <kernel args="kernel" />
    <dom>
        <app args="root">
            <dom>
                <app args="m3fs mem" daemon="1">
                    <serv name="m3fs" />
                    <mod name="fs" />
                </app>
            </dom>
            <dom>
                <app args="pager maxcli=3 sem=net" usermem="768M">
                    <sess name="m3fs" />
                    <mod name="fs" perm="r" />
                    <mod name="tilemux" perm="r" />
                    <tiles type="perf+nic" count="1" />
                    <tiles type="perf" count="2" />
                    <dom tile="perf">
                        <app args="/sbin/m3fs mem" daemon="1">
                            <serv lname="m3fs" gname="app_m3fs" />
                            <mod name="fs" />
                        </app>
                    </dom>
                    <dom tile="perf+nic">
                        <app args="/sbin/net net 192.168.41.11" daemon="1">
                            <serv name="net" />
                        </app>
                    </dom>
                    <dom tile="perf">
                        <app args="/bin/lvldbserver /tmp/foo 1 udp 192.168.41.10 1339 /data/small-workload.wl">
                            <sess name="net" args="bufs=1M socks=1" />
                            <sess lname="m3fs" gname="app_m3fs" />
                            <sem name="net" />
                        </app>
                    </dom>
                </app>
            </dom>
        </app>
    </dom>
</config>
