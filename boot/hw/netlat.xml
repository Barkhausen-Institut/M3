<config>
    <kernel args="kernel" />
    <dom>
        <app args="root">
            <dom>
                <app args="m3fs mem $fs.size" daemon="1">
                    <serv name="m3fs" />
                    <physmem addr="0" size="$fs.size" />
                </app>
            </dom>
            <dom>
                <app args="pager maxcli=2 $fs.size" usermem="768M">
                    <sess name="m3fs" />
                    <physmem addr="0" size="$fs.size" perm="r" />
                    <pes type="boom+nic" count="1" />
                    <pes type="boom" count="1" />
                    <dom pe="boom+nic">
                        <app args="/sbin/net net 192.168.42.242" daemon="1">
                            <serv name="net" />
                        </app>
                    </dom>
                    <dom pe="boom">
                        <app args="/bin/netlat 192.168.42.232 1338">
                            <sess name="net" args="bufs=1M socks=1 ports=2000" />
                        </app>
                    </dom>
                </app>
            </dom>
        </app>
    </dom>
</config>
