<config>
    <kernel args="kernel -f $fs.path" />
    <dom>
        <app args="root" />
    </dom>
    <dom>
        <app args="m3fs mem $fs.size" daemon="1">
            <serv name="m3fs" />
            <mem addr="0" size="$fs.size" />
        </app>
    </dom>
    <dom>
        <app args="pager /bin/bench-apps -r 1 4 /bin/fstrace-m3fs -p /tmp/1/ empty /bin/fstrace-m3fs -p /tmp/2/ empty">
            <sess name="m3fs" />
            <pes type="core" count="3" />
        </app>
    </dom>
</config>
